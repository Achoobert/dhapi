:"གསོལ་སྟོན་ཆེན་པོ་བཤམས་འདུག ","ct":"གསོལ་སྟོན་ཆེན་པོ་བཤམས་འདུག"}]},{"phrases":[{"at":"སུ་ཡང་འོང་འདོད་པ་རྣམས།།","kt":"སུ་ཡང་འོང་འདོད་པ་རྣམས། ","ct":"སུ་ཡང་འོང་འདོད་པ་རྣམས།"}]},{"phrases":[{"at":"ད་ལྟ་མགྱོགས་པོར་ཤོག་ཅིག། ༢","kt":"ད་ལྟ་མགྱོགས་པོར་ཤོག་ཅིག། ༢ ","ct":"ད་ལྟ་མགྱོགས་པོར་ཤོག་ཅིག ༢"}]}],"label":"verse 3","id":3},{"lines":[{"phrases":[{"at":"དཀོན་མཆོག་གི་སྲས་དེ་ལ།།","kt":" དཀོན་མཆོག་གི་སྲས་དེ་ལ། ","ct":"དཀོན་མཆོག་གི་སྲས་དེ་ལ།"}]},{"phrases":[{"at":"ཁྱོད་ཀྱི་སྡིག་པ་མཐོལ་ན།།","kt":"ཁྱོད་ཀྱི་སྡིག་པ་བཤགས་ན། ","ct":"ཁྱེད་ཀྱི་སྡིག་པ་བཤགས་ན།"}]},{"phrases":[{"at":"ཁོང་གིས་ཡོངས་སུ་བསལ་ཞིང་།།","kt":"ཁོང་གིས་ཡོངས་སུ་བསལ་ཞིང་། ","ct":"ཁོང་གིས་ཡོངས་སུ་བསལ་ཞིང་། "}]},{"phrases":[{"at":"མཐའ་མེད་པའི་སྲོག་གནང་ཡོང་།། ༢","kt":"མཐའ་མེད་པའི་སྲོག་གནང་ཡོང་།། ༢ ","ct":"མཐའ་མེད་པའི་སྲོག་གནང་ཡོང་།། ༢"}]}],"label":"verse 4","id":4},{"lines":[{"phrases":[{"at":"ཡིད་སེམས་ང་ལ་བཅོལ་ན།།","kt":" ཁྱོད་ཀྱི་སེམས་ང་ལ་གནང་། ","ct":"ཁྱེད་ཀྱི་སེམས་ང་ལ་གནང་།"}]},{"phrases":[{"at":"ངས་ཁྱོད་བདེ་སྐྱིད་གནང་ཡོང་།།","kt":"ངས་ཁྱོད་སྐྱིད་པོ་བྱེད་ཡོང་། ","ct":"ངས་ཁྱེད་སྐྱིད་པོ་བྱེད་ཡོང་།"}]},{"phrases":[{"at":"ཁོང་གིས་བྱམས་པའི་སྐད་ཀྱིས།།","kt":"ཁོང་གིས་བྱམས་པའི་སྐད་ཀྱིས། ","ct":"ཁོང་གིས་བྱམས་པའི་སྐད་ལ།"}]},{"phrases":[{"at":"ཁྱོད་ལ་འདི་ལྟར་བསྐུལ་ལོ།། ༢","kt":"ཁྱོད་ལ་འདི་ལྟར་བསྐུལ་ལོ།། ༢ ","ct":"ཁྱེད་ལ་བསྐུལ་མ་བཏང་ངོ་། ༢"}]}],"label":"verse 5","id":5},{"lines":[{"phrases":[{"at":"ྱོད་ཀྱི་ཡབ་ཀྱི་དྲུང་དུ།།","kt":"ཁྱོད་ཀྱི་ཡབ་ཀྱི་དྲུང་དུ། ","ct":"ྱེད་ཀྱི་ཡབ་ཀྱི་དྲུང་དུ།"}]},{"phrases":[{"at":"ད་ལྟ་མགྱོགས་པོར་ཤོག་ཅིག།","kt":"ད་ལྟ་མགྱོགས་པོར་ཤོག་ཅིག ","ct":"ད་ལྟ་མགྱོགས་པོར་ལོག་ཤོག "}]},{"phrases":[{"at":"ཁོང་ལ་ཁྱོད་འཇིགས་མི་དགོས།།","kt":"ཁོང་ལ་ཁྱོད་འཇིགས་མི་དགོས། ","ct":"ཁོང་ལ་ཁྱེད་འཇིགས་མི་དགོས།"}]},{"phrases":[{"at":"ཁོང་གིས་ཁྱོད་ལ་གཅེས་སོ།། ༢","kt":"ཁོང་གིས་ཁྱོད་ལ་གཅེས་སོ།། ༢ ","ct":"ཁོང་གིས་ཁྱེད་ལ་བྱམས་སོ། ༢"}]}],"label":"verse 6","id":6},{"lines":[{"phrases":[{"at":"ཡེ་ཤུའི་དྲུང་དུ་འོང་མཁན།།","kt":" ཡེ་ཤུའི་དྲུང་དུ་འོང་མཁན། ","ct":"ཡེ་ཤུའི་རྩ་ལ་འོང་མཁན།"}]},{"phrases":[{"at":"དེ་ཚོར་ཐར་བ་འཐོབ་ཡོང་།།","kt":"དེ